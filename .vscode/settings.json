{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["javascript", "typescript"],
  "prettier.requireConfig": true,
  "cSpell.words": [
    "conv",
    "frontends",
    "INITDB",
    "IOTP",
    "maxlength",
    "prefs",
    "tsparser",
    "virtuals",
    "Zync"
  ]
}
